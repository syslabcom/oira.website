<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Campaign Toolkit">

 <object name="campaigntoolkit" meta_type="Filesystem Directory View"
         directory="osha.campaigntoolkit:skins/campaigntoolkit"/>

 <object name="campaigntoolkitmobile" meta_type="Filesystem Directory View"
         directory="osha.campaigntoolkit:skins/campaigntoolkitmobile"/>


 <skin-path name="Campaign Toolkit" based-on="Sunburst Theme">
  <layer name="campaigntoolkit"
     insert-after="custom"/>
 </skin-path>

 <skin-path name="Campaign Toolkit Mobile" based-on="Go Mobile Default Theme">
  <layer name="campaigntoolkitmobile"
     insert-after="custom"/>
 </skin-path>

</object>
